.sql-cm-custom-tab-ul .z-tabs-cnt {
    border: none;
    background-color: #70c2d5;
}

.sql-cm-custom-tab-li div {
    background: none;
    padding: 0px;
}

.sql-cm-custom-tab-li .z-tab-text {
    font-size: 16px;
    font-family: Source Sans Pro-Semibold
    font-weight: normal;
    padding: 10px 15px 0px 15px;
    height: 25px;
    color: black;
}

.sql-cm-custom-tab-li .z-tab-text:hover {
    background-color: #006089;
	color: white;
}

.sql-cm-custom-tab-li .z-tab-hl:hover .z-tab-text {
    color: white;
}

.sql-cm-custom-tab-li.z-tab-seld .z-tab-text,
.sql-cm-custom-tab-li.z-tab-seld .z-tab-hl:hover .z-tab-text {
    color: #294148;
    font-weight: bold;
    border-bottom:5px solid #006089;
}